# WSL Paths
[ -f ~/.wsl-paths.sh ] && source ~/.wsl-paths.sh

# qnb
alias myqnb='cd ${WSL_QNB}'
[ -f ${WSL_QNB}/.sh/qnb-source-this.sh ] && source ${WSL_QNB}/.sh/qnb-source-this.sh

# f-mgmt v2
alias myfmgmt='cd ${WSL_F_MGMT}'
alias load-fmgmt='source ${WSL_F_MGMT}/.sh/f-mgmt-source-this.sh'

# alt+o workaround
alias ao='fzf-choose-locate-item | clip.exe'

# Etc...
alias vsc='vscode-open'                             # _wsl/.functions/better-commands-wsl.sh

# Open cwd in widows's explorer
alias exp='explorer.exe .'